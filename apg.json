{
 "variables": {
    "aws_access_key": "",
    "aws_secret_key": "",  
    "aws-region": "",
    "aws-vpc_id": "",
    "aws-subnet_id": ""
  },
 "builders": [{
  "type": "amazon-ebs",
  "ssh_pty": "true",
  "access_key": "{{user `aws_access_key`}}",
  "secret_key": "{{user `aws_secret_key`}}",
  "region": "{{user `aws-region`}}",
  "source_ami": "ami-f068a193",
  "instance_type": "t2.micro",
  "vpc_id": "{{user `aws-vpc_id`}}",
  "subnet_id": "{{user `aws-subnet_id`}}",
  "ssh_username": "centos",
  "ami_name": "packer-apg{{timestamp}}",
  "tags": {
  "Name": "unicornAMI",
  "Application": "DevOps"
}
}],
"provisioners": [{
  "type": "shell",
  "script": "apgscript.sh",
  "pause_before": "10s"
}]
}
