{
 "variables": {
    "aws_access_key": "",
    "aws_secret_key": "",
    "token": "",
    "aws-region": "",
    "aws-vpc_id": "",
    "aws-subnet_id": ""
  },
 "builders": [{
  "type": "amazon-ebs",
  "access_key": "{{user `aws_access_key`}}",
  "secret_key": "{{user `aws_secret_key`}}",
  "token": "{{user `aws_session_token`}}",
  "region": "{{user `aws-region`}}",
  "source_ami": "ami-8fcc75ec",
  "instance_type": "t2.micro",
  "vpc_id": "{{user `aws-vpc_id`}}",
  "subnet_id": "{{user `aws-subnet_id`}}",
  "ssh_username": "ubuntu",
  "ami_name": "APG-AMI",
  "tags": {
  "Name": "APG_AMI",
  "Application": "DevOps"
}
}],
"provisioners": [{
  "type": "shell",
  "script": "script.sh",
  "pause_before": "10s"
},
 {
  "type": "file",
  "source": "/home/ubuntu/config",
  "destination": "/tmp"
},
{
  "type": "file",
  "source": "/home/ubuntu/serv",
  "destination": "/tmp"
}
 ]
}
